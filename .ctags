--langdef=markdown
--langmap=markdown:.md.markdown.tmd
--regex-markdown=/^(# \S.*)/\1/h,h1/

--regex-markdown=/\/\/\s*(TODO.*$)/\1/h,todo/

--regex-markdown=/^(# \S.*)/\1/h,toc/
--regex-markdown=/^(## \S.*)/. . . . \1/h,toc/
--regex-markdown=/^(### \S.*)/. . . . . . . . \1/h,toc/

--langdef=txt2tags
--langmap=txt2tags:.t2t.txt2tags.txt

--regex-txt2tags=/^(TO-DO)/\1/h,todo/
--regex-txt2tags=/^(= \S.*)/\1/h,h1/
--regex-txt2tags=/^(== \S.*)/. \1/h,toc/
--regex-txt2tags=/^(=== \S.*)/. . \1/h,toc/